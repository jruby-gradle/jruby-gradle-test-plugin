<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>rubygems</groupId>
  <artifactId>rspec-expectations</artifactId>
  <version>3.3.1</version>
  <packaging>gem</packaging>
  <name>rspec-expectations-3.3.1</name>
  <url>http://github.com/rspec/rspec-expectations</url>
  <description>rspec-expectations provides a simple, readable API to express expected outcomes of a code example.</description>
  <developers>
    <developer>
      <name>Steven Baker</name>
      <email>rspec@googlegroups.com</email>
    </developer>
    <developer>
      <name>David Chelimsky</name>
    </developer>
    <developer>
      <name>Myron Marston</name>
    </developer>
  </developers>
  <scm>
    <connection>https://github.com/rspec/rspec-expectations.git</connection>
    <url>http://github.com/rspec/rspec-expectations</url>
  </scm>
  <properties>
    <project.build.sourceEncoding>utf-8</project.build.sourceEncoding>
    <jruby.plugins.version>1.0.4</jruby.plugins.version>
  </properties>
  <dependencies>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>rspec-support</artifactId>
      <version>[3.3.0,3.3.99999]</version>
      <type>gem</type>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>diff-lcs</artifactId>
      <version>[1.2.0,2.0)</version>
      <type>gem</type>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>rake</artifactId>
      <version>[10.0.0,10.0.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>cucumber</artifactId>
      <version>[1.3,1.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>aruba</artifactId>
      <version>[0.6,0.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>minitest</artifactId>
      <version>[5.2,5.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <extensions>
      <extension>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>gem-extension</artifactId>
        <version>${jruby.plugins.version}</version>
      </extension>
    </extensions>
    <directory>${basedir}/pkg</directory>
    <plugins>
      <plugin>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>gem-maven-plugin</artifactId>
        <version>${jruby.plugins.version}</version>
        <configuration>
          <gemspec>rspec-expectations-3.3.1.gemspec</gemspec>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
