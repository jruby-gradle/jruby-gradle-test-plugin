<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>rubygems</groupId>
  <artifactId>rspec-core</artifactId>
  <version>3.3.2</version>
  <packaging>gem</packaging>
  <name>rspec-core-3.3.2</name>
  <url>http://github.com/rspec/rspec-core</url>
  <description>BDD for Ruby. RSpec runner and example groups.</description>
  <developers>
    <developer>
      <name>Steven Baker</name>
      <email>rspec@googlegroups.com</email>
    </developer>
    <developer>
      <name>David Chelimsky</name>
    </developer>
    <developer>
      <name>Chad Humphries</name>
    </developer>
    <developer>
      <name>Myron Marston</name>
    </developer>
  </developers>
  <scm>
    <connection>https://github.com/rspec/rspec-core.git</connection>
    <url>http://github.com/rspec/rspec-core</url>
  </scm>
  <properties>
    <project.build.sourceEncoding>utf-8</project.build.sourceEncoding>
    <jruby.plugins.version>1.0.4</jruby.plugins.version>
  </properties>
  <dependencies>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>rspec-support</artifactId>
      <version>[3.3.0,3.3.99999]</version>
      <type>gem</type>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>rake</artifactId>
      <version>[10.0.0,10.0.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>cucumber</artifactId>
      <version>[1.3,1.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>minitest</artifactId>
      <version>[5.3,5.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>aruba</artifactId>
      <version>[0.6.2,0.6.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>nokogiri</artifactId>
      <version>[1.5,1.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>coderay</artifactId>
      <version>[1.0.9,1.0.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>mocha</artifactId>
      <version>[0.13.0,0.13.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>rr</artifactId>
      <version>[1.0.4,1.0.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>flexmock</artifactId>
      <version>[0.9.0,0.9.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>thread_order</artifactId>
      <version>[1.1.0,1.1.99999]</version>
      <type>gem</type>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <extensions>
      <extension>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>gem-extension</artifactId>
        <version>${jruby.plugins.version}</version>
      </extension>
    </extensions>
    <directory>${basedir}/pkg</directory>
    <plugins>
      <plugin>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>gem-maven-plugin</artifactId>
        <version>${jruby.plugins.version}</version>
        <configuration>
          <gemspec>rspec-core-3.3.2.gemspec</gemspec>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
